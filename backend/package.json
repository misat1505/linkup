{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx prisma db push && node ./build/app.js",
    "build": "tsup",
    "dev": "npx prisma db push && cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "e2e": "npx prisma db push && cross-env NODE_ENV=e2e ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "test": "npx jest --runInBand --forceExit",
    "test:coverage": "npx jest --coverage --runInBand --forceExit",
    "test:load": "npx artillery run ./tests/load-tests/test.yaml",
    "pretest:load:report": "npx mkdirp ./load-tests-report",
    "test:load:report": "npx artillery run --output ./load-tests-report/test-result.json ./tests/load-tests/test.yaml && npx artillery report ./load-tests-report/test-result.json",
    "docs": "typedoc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/s3-request-presigner": "^3.726.1",
    "@prisma/client": "^5.17.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.9.0",
    "@sentry/profiling-node": "^9.9.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-status-monitor": "^1.3.4",
    "http-status-codes": "^2.3.0",
    "i18next": "^24.2.3",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "rotating-file-stream": "^3.2.3",
    "sharp": "^0.33.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
