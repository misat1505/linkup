  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node src/app.ts",
    "dev": "cross-env NODE_ENV=development nodemon src/app.ts",
    "e2e": "cross-env NODE_ENV=e2e nodemon src/app.ts",
    "test": "npx dotenv -e .env.test -- npx prisma db push && cross-env NODE_ENV=test npx jest --runInBand --forceExit",
    "test:coverage": "npx dotenv -e .env.test -- npx prisma db push && cross-env NODE_ENV=test npx jest --coverage --runInBand --forceExit",
    "test:load": "npx artillery run ./tests/load-tests/test.yaml",
    "test:load:report": "npx artillery run --output ./load-tests-report/test-result.json ./tests/load-tests/test.yaml && npx artillery report ./load-tests-report/test-result.json"
  },
